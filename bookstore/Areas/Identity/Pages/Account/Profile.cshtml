@page
@model bookstore.Pages.Account.ProfileModel
@{
    ViewData["Title"] = "Profile";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h3>Your Profile</h3>
    <p><strong>Email:</strong> @Model.Email</p>
    <p><strong>Name:</strong> @Model.Name</p>
    <p><strong>Surname:</strong> @Model.Surname</p>
</div>
